<meta name="viewport" content="width=device-width, initial-scale=1">
<html>

<body>
    <%- include('partials/style.ejs') %>
    <%- include('partials/header.ejs') %>
    <div class="divider"></div>

    <div class="row section">
        <div class="col s12">
            <div class="col s12">
                <img class="brand-logo" src="images/logo.png" alt="Gitam Logo" width="300" height="100">
            </div>
        </div>
    </div>
<div class="divider"></div>
    <div class="container">
        <div class="row">
            <form action="login" method="POST">

                <div class="col s6  offset-s3">
                        <div class="section">
                    <h2> User Login </h2>
                        </div>
                </div>

                <div class="col s6  offset-s3 ">
                        <div class="section">
                    <input type="text" name="username" placeholder="Username" />
                        </div>
                </div>

                
                <div class="col s6 offset-s3 ">
                        <div class="section">
                    <input type="password" name="password" placeholder="Password" />
                </div>
            </div>

                <div class="col s6 offset-s3 center-align">
                        <div class="section">
                    <button class="waves-effect waves-light btn-large" onclick="validate()" type="submit">Submit</button>
                        </div>
                </div>
            </form>
        </div>
    </div>
</body>

</html>